          content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Use the title from a page's frontmatter if it has one -->
    <title>Xmas by Poulpe</title>
    <meta property="og:description" content="Agence de Création Digitale basée à Paris, Poulpe crée des expériences digitales innovantes et créatives."/>

    <meta property="og:type" content="article"/>
    <meta property="og:url" content="http://xmas.poulpe.co"/>
    <meta property="og:title" content="Xmas by Poulpe - Agence Digitale Agile"/>
    <meta property="og:image" content="http://xmas.poulpe.co/images/twentyfour.png"/>
    <meta property="og:description" content="Agence de Création Digitale basée à Paris, Poulpe crée des expériences digitales innovantes et créatives."/>
